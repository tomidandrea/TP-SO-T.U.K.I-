{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}",
                "/usr/local/include",
                "/usr/include"
            ],
            "defines": [
                "_GNU_SOURCE"
            ],
            "intelliSenseMode": "clang-x86",
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "/usr/local/include",
                    "/usr/include"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}